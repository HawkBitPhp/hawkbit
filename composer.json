{
  "name": "phpthinktank/proton",
  "type": "library",
  "description": "Micro PHP framework",
  "keywords": [
    "framework",
    "StackPHP",
    "HttpKernelInterface",
    "PSR-7",
    "middleware",
    "micro",
    "proton"
  ],
  "homepage": "https://github.com/phpthinktank/proton",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Bilbie",
      "email": "hello@alexbilbie.com"
    },
    {
      "name": "Marco Bunge",
      "email": "marco_bunge@web.de"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "league/route": "dev-develop",
    "league/container": "~2.0",
    "league/event": "~2.1",
    "monolog/monolog": "~1.12",
    "zendframework/zend-diactoros": "~1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "symfony/http-kernel": "~2.7|~3.0",
    "symfony/psr-http-message-bridge": "*"
  },
  "autoload": {
    "psr-4": {
      "Proton\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProtonTests\\": "tests/"
    },
    "files": [
      "tests/TestAsset/Functions.php",
      "tests/TestAsset/SapiResponse.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "master"
    }
  },
  "provide": {
    "psr/http-message-implementation": "~1.0",
    "container-interop/container-interop-implementation": "^1.1"
  },
  "suggest": {
    "symfony/http-kernel": "Need for symfony http foundation adapter",
    "symfony/psr-http-message-bridge": "Need for symfony http foundation adapter",
    "zendframework/zend-diactoros": "Need for symfony http foundation adapter"
  }
}
